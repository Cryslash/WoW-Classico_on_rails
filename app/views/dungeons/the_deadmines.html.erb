<title>The Deadmines</title>
<section>

<div class="right_side">
   <div class="rs_top" id="deadmines_map">
   </div>
   <div class="rs_bottom">
     <ol>
        <li><span><%= t "others.name" %>:</span> <%= t "dungeons_name.The Deadmines" %>.</li>
        <li><span><%= t "others.level" %>:</span> 16 - 22.</li>
        <li><span><%= t "others.place" %>:</span> <%= t "locales.eastern_kingdoms" %>, <%= t "locales.westfall" %>.</li>
     </ol>
   </div>
</div>

<div class="body" id="deadmines_bg">
    <ol>
       <li><%= t "dungeons.template_title" %> <%= t "dungeons_name.The Deadmines" %>.</li>
    </ol>

<%for i in 0..4%>
 <div class="quests">
   <div class="quest_text">
     <ul>
          <li><span class="line1">[<%=@q[i][0]%>] - [<a href="<%=@q[i][1] %>"></a>]</span></li>
       <%if @q[i][2] != ""%>
          <li><%= t "quests.quest_begin"%> <a href="<%=@q[i][2] %>"></a> <%= t "quests.in" %> <%= t "locales.#{@q[i][3]}"%>.</li>
       <%end%>
      <%if @q[i][6] != ""%>
          <li> <%= t "quests.quest_chain" %> <a href="<%=@q[i][5]%>"></a> <%= t "quests.quest_chain_begin" %> <a href="<%=@q[i][6]%>"></a> <%= t "quests.in" %> <%= t "locales.#{@q[i][7]}"%>.</li>
      <%end%>
     </ul>
   </div>
<div class="<%=@q[i][4]%>"></div>
</div>
<%end%>

</section>