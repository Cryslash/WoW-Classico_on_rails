<title>Maraudon</title>
<section>

<div class="right_side">
   <div class="rs_top" id="maraudon_map">
     
   </div>
   <div class="rs_bottom">
     <ol>            
        <li><span>Nome:</span>Maraudon</li>
        <li><span>Nível:</span> 42 - 51</li>
        <li><span>Local:</span> Kalimdor, Desolação.</li>
     </ol>
   </div>
</div>
<div class="body" id="maraudon_bg">
       <ol>            
          <li>Missões para fazer em: Maraudon.</li>            
       </ol>

<%for i in 0..7%>
 <div class="quests">
   <div class="quest_text">
     <ul>
         <li><span class="line1">[<%=@q[i][0]%>] - [<a href="<%=@q[i][1] %>"></a>]</span></li>
       <%if @q[i][5] == ""%>
         <li>A missão inicia com <a href="<%=@q[i][2] %>"></a> em: <%=@q[i][3]%>.</li>
       <%end%>
       <%if @q[i][5] != ""%>
         <li>A missão inicia com <a href="<%=@q[i][2]%>"></a> <%=@q[i][3]%> <a href="<%=@q[i][5]%>"></a> <%=@q[i][6]%> </li>
       <%end%>
     </ul>
   </div>
<div class="<%=@q[i][4]%>"></div>
</div> 
<%end%>

 </section>	