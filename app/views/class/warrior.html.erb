  <title>Guerreiro • WoW-Clássico</title>

  <section>
  <div class="card" id="warrior_card">
      <div class="container">             
            <%= image_tag "classes/warrior-128"%>
            <h1><%= t "class.text" %><%= t "class_name.warrior" %></h1>
            
            <div class="swiper-container">
            <div class="swiper-wrapper">

                 <% for i in 0..5 %>
                 <div class="swiper-slide">                   
                   <%= link_to "#{t"others.tap_me"}",@warrior_builds[i][0]%>
